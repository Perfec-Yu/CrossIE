        "ar": "\u0627\u0633\u062a\u0642\u0628\u0644\u062a \u0627\u0644\u0642\u0627\u0647\u0631\u0629 \u0639\u0644\u0649 \u0645\u062f\u0627\u0631 \u0627\u0644\u0633\u0627\u0639\u0627\u062a \u0627\u0644\u0645\u0627\u0636\u064a\u0629 \u0627\u0644\u0639\u062f\u064a\u062f \u0645\u0646 \u0627\u0644\u0632\u0639\u0645\u0627\u0621 \u0648\u0627\u0644\u0642\u0627\u062f\u0629 \u0627\u0644\u0639\u0631\u0628 \u0627\u0644\u0630\u064a\u0646 \u062a\u0623\u062a\u064a \u0632\u064a\u0627\u0631\u062a\u0647\u0645 \u0641\u064a \u0625\u0637\u0627\u0631 \u0627\u0644\u0645\u0634\u0627\u0631\u0643\u0629 \u0641\u064a \u0627\u062c\u062a\u0645\u0627\u0639\u0627\u062a \u0627\u0644\u0642\u0645\u0629 \u0627\u0644\u0639\u0631\u0628\u064a\u0629 \u0627\u0644\u0637\u0627\u0631\u0626\u0629 \u0648\u0627\u0644\u062a\u064a \u062f\u0639\u0627 \u0625\u0644\u064a\u0647\u0627 \u0627\u0644\u0631\u0626\u064a\u0633 \u062d\u0633\u0646\u064a \u0645\u0628\u0627\u0631\u0643 \u0648\u0642\u062f \u0643\u0627\u0646 \u0627\u0644\u0631\u0626\u064a\u0633 \u0645\u0628\u0627\u0631\u0643 \u0639\u0644\u0649 \u0631\u0623\u0633 \u0645\u0633\u062a\u0642\u0628\u0644\u064a \u0627\u0644\u0632\u0639\u0645\u0627\u0621 \u0648\u0627\u0644\u0642\u0627\u062f\u0629 \u0627\u0644\u0639\u0631\u0628 \u0627\u0644\u0630\u064a\u0646 \u062d\u0636\u0631\u0648\u0627 \u0625\u0644\u0649 \u0627\u0644\u0642\u0627\u0647\u0631\u0629 \u064a\u062d\u0630\u0648\u0647\u0645 \u0627\u0644\u0623\u0645\u0644 \u0641\u064a \u062a\u062d\u0642\u064a\u0642 \u0645\u062c\u0645\u0648\u0639\u0629 \u0645\u0646 \u0627\u0644\u0623\u0647\u062f\u0627\u0641 \u0627\u0644\u062a\u064a \u062a\u062f\u0639\u0645 \u0627\u0644\u062a\u0636\u0627\u0645\u0646 \u0627\u0644\u0639\u0631\u0628\u064a \u0648\u062a\u0639\u064a\u062f \u0627\u0644\u062d\u0642\u0648\u0642 \u0627\u0644\u0639\u0631\u0628\u064a\u0629 \u0627\u0644\u0645\u063a\u062a\u0635\u0628\u0629 \u0625\u0644\u0649 \u0623\u0647\u0644\u0647\u0627 \u0648\u0642\u062f \u0643\u0627\u0646 \u062a\u0635\u0631\u064a\u062d\u0627\u062a \u0627\u0644\u0632\u0639\u0645\u0627\u0621 \u0641\u0648\u0631 \u0648\u0642\u0628\u064a\u0644 \u0648\u0635\u0648\u0644\u0647\u0645 \u0625\u0644\u0649 \u0627\u0644\u0642\u0627\u0647\u0631\u0629 \u062a\u0624\u0643\u062f \u062a\u0644\u0643 \u0627\u0644\u0623\u0647\u062f\u0627\u0641 \u0641\u0644\u062f\u0649 \u0648\u0635\u0648\u0644\u0647 \u0625\u0644\u0649 \u0627\u0644\u0642\u0627\u0647\u0631\u0629 \u0623\u0639\u0644\u0646 \u0627\u0644\u0631\u0626\u064a\u0633 \u0627\u0644\u0641\u0644\u0633\u0637\u064a\u0646\u064a \u064a\u0627\u0633\u0631 \u0639\u0631\u0641\u0627\u062a \u0623\u0646 \u0627\u062a\u0641\u0627\u0642 \u0634\u0631\u0645 \u0627\u0644\u0634\u064a\u062e \u0647\u0648 \u0627\u0644\u0645\u0648\u0636\u0648\u0639 \u0627\u0644\u0623\u0633\u0627\u0633\u064a \u0627\u0644\u0630\u064a \u0633\u062a\u0628\u062d\u062b\u0647 \u0627\u0644\u0642\u0645\u0629 \u0627\u0644\u0639\u0631\u0628\u064a\u0629 \u0627\u0644\u0637\u0627\u0631\u0626\u0629 \u0648\u0643\u0623\u0646 \u0627\u0644\u0631\u0626\u064a\u0633 \u0639\u0631\u0641\u0627\u062a \u064a\u0631\u062f \u0628\u0630\u0644\u0643 \u0644\u062f\u0649 \u0648\u0635\u0648\u0644\u0647 \u0625\u0644\u0649 \u0645\u0642\u0631 \u0625\u0642\u0627\u0645\u062a\u0647 \u0641\u064a \u0627\u0644\u0642\u0627\u0647\u0631\u0629 \u0639\u0644\u0649 \u0633\u0624\u0627\u0644 \u0644\u0635\u062d\u064a\u0641\u0629 \u0641\u0644\u0633\u0637\u064a\u0646\u064a\u0629 \u0639\u0645\u0627 \u0625\u0630\u0627 \u0643\u0627\u0646 \u0627\u062a\u0641\u0627\u0642 \u0634\u0631\u0645 \u0627\u0644\u0634\u064a\u062e \u0645\u0627 \u0632\u0627\u0644 \u062d\u064a\u0627 \u0648\u0642\u062f \u0648\u0635\u0644 \u0625\u0644\u0649 \u0645\u0637\u0627\u0631 \u0627\u0644\u0642\u0627\u0647\u0631\u0629 \u0623\u064a\u0636\u0627 \u0627\u0644\u0631\u0626\u064a\u0633 \u0627\u0644\u0633\u0648\u0631\u064a \u0628\u0634\u0627\u0631 \u0627\u0644\u0623\u0633\u062f \u0644\u062d\u0636\u0648\u0631 \u0627\u062c\u062a\u0645\u0627\u0639\u0627\u062a \u0627\u0644\u0642\u0645\u0629 \u0627\u0644\u0639\u0631\u0628\u064a\u0629 \u0648\u0643\u0627\u0646\u062a \u0633\u0648\u0631\u064a\u0629 \u0642\u062f \u0623\u0643\u062f\u062a \u0623\u0646 \u0627\u0644\u0642\u0645\u0629 \u0627\u0644\u0639\u0631\u0628\u064a\u0629 \u062a\u0639\u0645\u0644 \u0639\u0644\u0649 \u0627\u062a\u062e\u0627\u0630 \u0627\u0644\u0642\u0631\u0627\u0631\u0627\u062a \u0627\u0644\u0645\u0647\u0645\u0629 \u0627\u0644\u062a\u064a \u0645\u0646 \u0634\u0623\u0646\u0647\u0627 \u062a\u0639\u0632\u064a\u0632 \u0627\u0644\u062a\u0636\u0627\u0645\u0646 \u0627\u0644\u0639\u0631\u0628\u064a \u0648\u062a\u062d\u0645\u064a\u0644 \u0625\u0633\u0631\u0627\u0626\u064a\u0644 \u0643\u0644 \u0627\u0644\u0645\u0633\u0626\u0648\u0644\u064a\u0629 \u0625\u064a\u0630\u0627\u0621 \u0645\u0627 \u064a\u062d\u062f\u062b \u0645\u0646 \u062a\u062f\u0647\u0648\u0631 \u0641\u064a \u0639\u0645\u0644\u064a\u0629 \u0627\u0644\u0633\u0644\u0627\u0645 \u0648\u0642\u0627\u0644\u062a \u0633\u0648\u0631\u064a\u0629 \u0639\u0644\u0649 \u0644\u0633\u0627\u0646 \u0648\u0632\u064a\u0631 \u062e\u0627\u0631\u062c\u064a\u062a\u0647\u0627 \u0641\u0627\u0631\u0648\u0642 \u0627\u0644\u0634\u0631\u0639 \u0625\u0646 \u0637\u0645\u0648\u062d\u0627\u062a \u0633\u0648\u0631\u064a\u0627 \u0643\u0628\u064a\u0631\u0629 \u0625\u0644\u0627 \u0623\u0646\u0647\u0627 \u0641\u064a \u0627\u0644\u0648\u0642\u062a \u0646\u0641\u0633\u0647 \u062a\u0623\u062e\u0630 \u0641\u064a \u0627\u0644\u0627\u0639\u062a\u0628\u0627\u0631 \u0638\u0631\u0648\u0641 \u0627\u0644\u0623\u0634\u0642\u0627\u0621 \u0627\u0644\u0639\u0631\u0628 \u0645\u0634\u064a\u0631\u0627 \u0625\u0644\u0649 \u0623\u0646 \u0627\u0644\u062a\u0637\u0628\u064a\u0639 \u0645\u0639 \u0625\u0633\u0631\u0627\u0626\u064a\u0644 \u064a\u0639\u062f \u0648\u0631\u0642\u0629 \u0642\u0648\u064a\u0629 \u0641\u064a \u0623\u064a\u062f\u064a \u0627\u0644\u062d\u0643\u0648\u0645\u0627\u062a \u0627\u0644\u0639\u0631\u0628\u064a\u0629 \u064a\u062c\u0628 \u0623\u0646 \u062a\u0633\u062a\u063a\u0644 \u062c\u064a\u062f\u0627 \u0643\u0645\u0627 <a>\u0648\u0635\u0644</a>  <b>\u0644\u0644\u0642\u0627\u0647\u0631\u0629</b> \u0627\u0644\u0631\u0626\u064a\u0633 \u0627\u0644\u0644\u0628\u0646\u0627\u0646\u064a \u0627\u0644\u0639\u0645\u0627\u062f \u0625\u0645\u064a\u0644 \u0644\u062d\u0648\u062f \u0644\u0644\u0645\u0634\u0627\u0631\u0643\u0629 \u0641\u064a \u0627\u0644\u0642\u0645\u0629 \u0627\u0644\u0639\u0631\u0628\u064a\u0629 \u0648\u0630\u0643\u0631 \u0628\u064a\u0627\u0646 \u0644\u0631\u0626\u0627\u0633\u0629 \u0627\u0644\u062c\u0645\u0647\u0648\u0631\u064a\u0629 \u0627\u0644\u0644\u0628\u0646\u0627\u0646\u064a\u0629 \u0623\u0646 \u0627\u0644\u0648\u0641\u062f \u0627\u0644\u0644\u0628\u0646\u0627\u0646\u064a \u0627\u0644\u0631\u0633\u0645\u064a \u0641\u064a \u0647\u0630\u0647 \u0627\u0644\u0642\u0645\u0629 \u0648\u0627\u0644\u0630\u064a \u064a\u062a\u0631\u0623\u0633\u0647 \u0627\u0644\u0631\u0626\u064a\u0633 \u0644\u062d\u0648\u062f \u064a\u0636\u0645 \u0627\u0644\u062f\u0643\u062a\u0648\u0631 \u0633\u0644\u064a\u0645 \u0627\u0644\u062d\u0635 \u0631\u0626\u064a\u0633 \u0627\u0644\u0648\u0632\u0631\u0627\u0621 \u0648\u0639\u062f\u062f\u0627 \u0645\u0646 \u0643\u0628\u0627\u0631 \u0627\u0644\u0645\u0633\u0626\u0648\u0644\u064a\u0646 \u0627\u0644\u0644\u0628\u0646\u0627\u0646\u064a\u064a\u0646 \u0648\u0642\u062f \u0639\u0643\u0633 \u0627\u0644\u0631\u0626\u064a\u0633 \u0644\u062d\u0648\u062f \u062e\u0644\u0627\u0644 \u0627\u0644\u064a\u0648\u0645\u064a\u0646 \u0627\u0644\u0645\u0627\u0636\u064a\u064a\u0646 \u0639\u0644\u0649 \u062f\u0631\u0627\u0633\u0629 \u0648\u0625\u0639\u062f\u0627\u062f \u0627\u0644\u0645\u0644\u0641 \u0627\u0644\u0644\u0628\u0646\u0627\u0646\u064a \u0627\u0644\u0630\u064a \u0633\u064a\u0637\u0631\u062d \u0639\u0644\u0649 \u0627\u0644\u0642\u0645\u0629 \u0648\u064a\u062a\u0646\u0627\u0648\u0644 \u0648\u062c\u0647\u0629 \u0627\u0644\u0646\u0638\u0631 \u0627\u0644\u0644\u0628\u0646\u0627\u0646\u064a\u0629 \u062a\u062c\u0627\u0647 \u062c\u0645\u064a\u0639 \u0627\u0644\u0642\u0636\u0627\u064a\u06